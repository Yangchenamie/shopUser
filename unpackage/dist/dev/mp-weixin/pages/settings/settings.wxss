.tabNav {
  position: relative;
  width: 100%;
  line-height: 92rpx;
  text-align: center;
  padding: 0 40rpx;
  box-sizing: border-box;
  color: #000;
  font-size: 40rpx;
  border-bottom: 20rpx solid #F1F1F1;
}
.tabNav .tabImg {
  position: absolute;
  left: 20px;
  width: 40rpx;
  height: 40rpx;
}
.tabNav .tabImg image {
  width: 100%;
  height: 100%;
}
.con {
  width: 100%;
}
.con .con-item {
  display: flex;
  justify-content: space-between;
  padding: 50rpx 66rpx 20rpx;
  border-bottom: 2rpx solid #f1f1f1;
  box-sizing: border-box;
  vertical-align: bottom;
}
.con .con-item image {
  width: 100rpx;
  height: 100rpx;
}
.con .con-item .item-fl {
  color: #000;
  font-size: 40rpx;
}
.con .con-item .item-fr {
  color: #595959;
}
.con .con-item .photo {
  margin-top: 50rpx;
}

